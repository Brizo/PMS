                                              <form>
                                                <textarea class="col-sm-12" rows="4" name="rejectObj" 
                                                ng-model="rejCurrComment">
                                                  {{rejCurrComment}}
                                                </textarea>
                                                
                                              </form>
                                              <br>
                                          </div>
                                        </div>
                                        <div id="" class="modal-footer">
                                          <button class="btn btn-small btn-success" data-backdrop="false" data-dismiss="modal" ng-disabled="disabledReject" ng-click="rejCurrObj(objective._id, rejCurrComment)" onclick="$('.modal-backdrop').css({'position': 'relative'});"><span class="glyphicon glyphicon-ok-circle"> </span>&nbsp;Finish </button>
                                          <button data-backdrop="false" class="btn btn-small btn-danger" data-dismiss="modal"><span class="glyphicon glyphicon-remove"></span>&nbsp;Close</button>
                                        </div>
                                      </div>
                                    </div>
                                  </div> 

                              </td>
                            </tr>
                          </tbody>
                        </form> 
                      </table> 
